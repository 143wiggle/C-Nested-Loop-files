#include <stdio.h>

int main() {
    int number, i;

    // Ask user for the number to print the multiplication table
    printf("Enter a number: ");
    scanf("%d", &number);

    // Loop to print the multiplication table
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", number, i, number * i);  // Prints the multiplication result
    }

    return 0;
}
