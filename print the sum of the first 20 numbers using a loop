#include <stdio.h>

int main() {
    int i, sum = 0;

    // Loop to calculate the sum of the first 20 numbers
    for (i = 1; i <= 20; i++) {
        sum += i;  // Adds the current value of i to sum
    }

    printf("The sum of the first 20 numbers is: %d\n", sum);  // Prints the sum
    return 0;
}
